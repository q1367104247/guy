//对象数组
//一个项目中，直接调用Cat类
package test03.pojo;

public class Test03 {

	
	
	

	public static void main(String[] args) {
		Cat cat1=new Cat("大傻","黑色",2);
		Cat cat2=new Cat("二傻","红色",2);
		Cat cat3=new Cat("三傻","白色",2);
		Cat cat4=new Cat("四傻","花色",2);
		
		Cat[] catArray={cat1,cat2,cat3,cat4};
		for(int i=0;i<catArray.length;i++){
			catArray[i].show();
		}
		
		for(Cat c:catArray){
			c.show();
		}

	}

}
